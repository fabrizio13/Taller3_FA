<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a16866099_5.taller3_fa.Fragments.DataFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/buttonSend2"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:layout_alignBottom="@+id/textView2"
            android:layout_alignStart="@+id/buttonSend3"
            android:text="Ver"
            android:layout_alignLeft="@+id/buttonSend" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="141dp"
            android:text="-> Está en DICOM :D"
            android:layout_alignParentLeft="true" />

        <Button
            android:id="@+id/buttonSend"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/textView"
            android:layout_marginEnd="22dp"
            android:text="Ver"
            android:layout_alignParentRight="true"
            android:layout_marginRight="22dp" />


        <TextView
            android:id="@+id/textView"
            android:layout_width="289dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="67dp"
            android:text="-> Últimos días La Polar"
            android:layout_alignParentLeft="true" />

        <Button
            android:id="@+id/buttonSend3"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/textView3"
            android:layout_marginEnd="22dp"
            android:text="Ver"
            android:layout_alignParentRight="true"
            android:layout_marginRight="22dp" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="289dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="209dp"
            android:text="-> Su saldo ha expirado" />
    </RelativeLayout>
</FrameLayout>